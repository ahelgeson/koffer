        android:layout_height="wrap_content"
        android:id="@+id/button6" />

    <Button
        android:text="@string/op_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button7" />

    <Button
        android:text="@string/op_11"
